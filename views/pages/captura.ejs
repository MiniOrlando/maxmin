                                                <% var date = new Date(data.tabla[i].fec_cre); %>
                                                <% var day = date.getDate(); %>
                                                <% var month = date.getMonth() + 1; %>
                                                <% var year = date.getFullYear(); %>
                                                <% day = (day < 10 ? "0" : "") + day; %>
                                                <% month = (month < 10 ? "0" : "") + month; %>
                                                <% var formattedDate = day + "/" + month + "/" + year;  %>

                                                <% var url = ''; %>
                                                <% var tipo = data.tabla[i].tipo_art.trim(); %>
                                                <% if (tipo == 'PAN') { %>
                                                    <% url = 'panes/panes_500.png'; %>
                                                <% } else if (tipo == 'FRUTA') { %>
                                                    <% url = 'frutas/frutas_500.png'; %>
                                                <% } else if (tipo == 'TORTILLAS') { %>
                                                    <% url = 'tortillas/tortillas_sueltas_500.png'; %>
                                                <% } else if (tipo == 'VERDURAS') { %>
                                                    <% url = 'verduras/verduras_500.png'; %>
                                                <% } %>


